{
  "name": "flux-eco/open-api-client",
  "description": "Component for flux-capacitor apps developed by fluxlabs",
  "version": "0.0.1",
  "type": "flux-app",
  "keywords": [
    "flux-ecosystem",
    "flux-capacitor",
    "fluxlabs ag",
    "microservice",
    "event sourcing",
    "cqrs",
    "json schema",
    "asyncapi",
    "openapi"
  ],
  "homepage": "https://fluxlabs.ch",
  "license": "GPL-3.0-only",
  "authors": [
    {
      "name": "fluxlabs ag",
      "email": "support@fluxlabs.ch",
      "homepage": "https://fluxlabs.ch",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "support@fluxlabs.ch"
  },
  "require": {
    "php": ">=8.0",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-yaml": "*",
    "composer-runtime-api": ">=2.1"
  },
  "require-dev": {
    "composer/composer": ">=2.0",
    "swoole/ide-helper": ">=4.6"
  },
  "autoload": {
    "psr-4": {
      "FluxEco\\OpenApiClient\\": [
        "src/"
      ]
    }
  },
  "config": {
    "classmap-authoritative": true,
    "optimize-autoloader": true,
    "sort-packages": true,
    "platform-check": true,
    "allow-plugins": {
      "composer/package-versions-deprecated": true
    }
  }
}
